        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:aoe3wpfg"
        mc:Ignorable="d"
        Title="uinewdialogs"
        Style="{StaticResource WindowStyle}">
    <Viewbox Style="{StaticResource ViewboxMain}">
        <Grid x:Name="mainGridPanel" Width="1920" Height="1080">
            
            <ContentControl Template="{StaticResource ScreenBlur}" />
            
            <Frame Name="MainFrame" Focusable="False" NavigationUIVisibility="Hidden" Source="pages\uinewdialogs\YouAreVictoriousDialog.xaml" />

        </Grid>
    </Viewbox>
</Window>
