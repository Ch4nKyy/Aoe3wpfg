<Window x:Class="aoe3wpfg.uicredits" xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:d="http://schemas.microsoft.com/expression/blend/2008" xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" xmlns:local="clr-namespace:aoe3wpfg" mc:Ignorable="d" Title="uicredits" Style="{StaticResource WindowStyle}">
    <Window.Resources>
        <ControlTemplate TargetType="ContentControl" x:Key="SmokeyBG">
            <Grid Width="5760" Height="1080">
                <Rectangle x:Name="Smoke1" Style="{StaticResource SmokeRect}" VerticalAlignment="Bottom" HorizontalAlignment="Left" Width="5760" Margin="0" Opacity="0.2">
                    <Rectangle.Fill>
                        <ImageBrush ImageSource="/resources/images/campaignmenu/smoke.png" Stretch="UniformToFill" TileMode="Tile" Viewport="0,0,1920,512" ViewportUnits="Absolute" />
                    </Rectangle.Fill>
                </Rectangle>

                <Rectangle x:Name="Smoke2" Style="{StaticResource SmokeRect}" VerticalAlignment="Bottom" HorizontalAlignment="Left" Width="5760" Margin="0" Opacity="0.1">
                    <Rectangle.Fill>
                        <ImageBrush ImageSource="/resources/images/campaignmenu/smoke.png" Stretch="UniformToFill" TileMode="Tile" Viewport="0,0,1920,512" ViewportUnits="Absolute" />
                    </Rectangle.Fill>
                </Rectangle>

                <Rectangle x:Name="Smoke3" Style="{StaticResource SmokeRect}" VerticalAlignment="Bottom" HorizontalAlignment="Left" Width="5760" Margin="0,0,0,-150" Opacity="0.3">
                    <Rectangle.Fill>
                        <ImageBrush ImageSource="/resources/images/campaignmenu/smoke.png" Stretch="UniformToFill" TileMode="Tile" Viewport="0,0,1920,512" ViewportUnits="Absolute" />
                    </Rectangle.Fill>
                </Rectangle>

            </Grid>
            <ControlTemplate.Triggers>
                <Trigger Property="IsEnabled" Value="True">
                    <Trigger.EnterActions>
                        <BeginStoryboard>
                            <Storyboard>
                                <DoubleAnimation Storyboard.TargetName="Smoke1" Storyboard.TargetProperty="RenderTransform.(TranslateTransform.X)" From="0" BeginTime="0:0:0" To="-1920" Duration="0:1:0" RepeatBehavior="Forever" />
                                <DoubleAnimation Storyboard.TargetName="Smoke2" Storyboard.TargetProperty="RenderTransform.(TranslateTransform.X)" From="0" BeginTime="-0:2:0" To="-1920" Duration="0:5:0" RepeatBehavior="Forever" />
                                <DoubleAnimation Storyboard.TargetName="Smoke3" Storyboard.TargetProperty="RenderTransform.(TranslateTransform.X)" From="-1920" BeginTime="0:0:0" To="0" Duration="0:1:0" RepeatBehavior="Forever" />
                            </Storyboard>
                        </BeginStoryboard>
                    </Trigger.EnterActions>
                </Trigger>
            </ControlTemplate.Triggers>
        </ControlTemplate>
        <Style TargetType="TextBlock" x:Key="creditsName">
            <Setter Property="FontFamily" Value="Trajan Pro" />
            <Setter Property="FontSize" Value="21" />
            <Setter Property="Foreground" Value="White" />
            <Setter Property="HorizontalAlignment" Value="Center" />
            <Setter Property="VerticalAlignment" Value="Center" />
            <Setter Property="Margin" Value="0,5,0,5"/>
            <Setter Property="SnapsToDevicePixels" Value="False"/>
        </Style>
        <Style TargetType="TextBlock" x:Key="creditsTitle">
            <Setter Property="FontFamily" Value="Trajan Pro" />
            <Setter Property="FontWeight" Value="Bold" />
            <Setter Property="FontSize" Value="31" />
            <Setter Property="Foreground" Value="{StaticResource ButtonTextGradient}" />
            <Setter Property="HorizontalAlignment" Value="Center" />
            <Setter Property="VerticalAlignment" Value="Center" />
            <Setter Property="TextAlignment" Value="Center"/>
            <Setter Property="Margin" Value="0,10,0,10"/>
            <Setter Property="SnapsToDevicePixels" Value="False"/>
        </Style>
        <Style TargetType="Rectangle" x:Key="SmokeRect">
            <Setter Property="Height" Value="512" />
            <Setter Property="Width" Value="3840" />
            <Setter Property="Margin" Value="0,50" />
            <Setter Property="RenderTransformOrigin" Value="0.5, 0.5" />
            <Setter Property="RenderTransform">
                <Setter.Value>
                    <ScaleTransform ScaleX="1" ScaleY="1" />
                </Setter.Value>
            </Setter>
            <Setter Property="RenderTransform">
                <Setter.Value>
                    <TranslateTransform X="0" Y="0" />
                </Setter.Value>
            </Setter>
        </Style>
        <ImageBrush x:Key="credits_bg" ImageSource="/resources/images/credits/credits_bg.png" Stretch="UniformToFill" />
    </Window.Resources>

    <Viewbox Style="{StaticResource ViewboxMain}">
        <Grid x:Name="mainGridPanel" Width="1920" Height="1080" Background="{StaticResource credits_bg}">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="200" />
                <ColumnDefinition Width="*" />
                <ColumnDefinition Width="200" />
            </Grid.ColumnDefinitions>

            <ScrollViewer Grid.Column="1" VerticalAlignment="Stretch" Margin="0,105,0,55" VerticalScrollBarVisibility="Hidden">

                <Canvas Grid.Column="1" HorizontalAlignment="Center" VerticalAlignment="Stretch" Width="1400">
                    <StackPanel x:Name="creditsPanel" Width="1400" Margin="0,0,0,0" SnapsToDevicePixels="False">
                        <StackPanel.RenderTransform>
                            <TransformGroup>
                                <ScaleTransform ScaleY="-1"/>
                                <TranslateTransform x:Name="CreditsOffset" Y="{Binding ElementName=creditsPanel, Path=ActualHeight}"/>
                                <ScaleTransform ScaleY="-1"/>
                            </TransformGroup>
                        </StackPanel.RenderTransform>
                        <StackPanel.Triggers>
                            <EventTrigger RoutedEvent="Loaded">
                                <BeginStoryboard>
                                    <Storyboard x:Name="CreditsStoryboard" FillBehavior="Stop" >
                                        <DoubleAnimation Storyboard.TargetName="CreditsOffset" Duration="0:0:1" Storyboard.TargetProperty="Y" From="-1080" To="-950" DecelerationRatio="1"/>
                                        <DoubleAnimation Storyboard.TargetName="CreditsOffset" Duration="0:8:0" BeginTime="0:0:1" Storyboard.TargetProperty="Y" From="-950"/>
                                    </Storyboard>
                                </BeginStoryboard>
                            </EventTrigger>
                        </StackPanel.Triggers>
                        
